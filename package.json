{
  "name": "electron-call",
  "version": "0.1.0",
  "description": "The easiest main-renderer IPC communication",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "node build.mjs",
    "lint": "eslint . && prettier . --check",
    "test": "vitest run src",
    "test:e2e": "npm run build && vitest run e2e --disable-console-intercept --reporter=basic",
    "test:full": "npm test && npm run lint && tsc --noEmit  && npm run test:e2e",
    "postversion": "git push && git push --tags",
    "prepack": "npm run build && npm run test:full",
    "preversion": "npm run test:full"
  },
  "repository": "megahertz/electron-call",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "electron",
    "ipc",
    "rpc",
    "proxy"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/electron-call/issues",
  "homepage": "https://github.com/megahertz/electron-call#readme",
  "engines": {
    "node": ">=16.0"
  },
  "devDependencies": {
    "@types/node": "^18.19.39",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "dts-bundle-generator": "^9.5.1",
    "electron": "^31.1.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "humile": "^0.3.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  }
}
